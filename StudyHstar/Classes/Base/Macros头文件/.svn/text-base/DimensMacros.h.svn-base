#ifndef CarAppBuyers_DimensMacros_h
#define CarAppBuyers_DimensMacros_h



/**
 *  TabBar高度
 */
#define KTabBarHeight 49.0f
/**
 *  NavigationBar高度
 */
#define KNavigationBarHeight 44.0f
/**
 *  热点栏高度
 */
#define KHotSpotBarHeight 20.0f
/**
 *  状态栏高度
 */
#define KStatuBarHeight 20.0f
